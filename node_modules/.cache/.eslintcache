=\"objective-container\">\n          <div className=\"objectiveTab\">\n            {tabs.map((tab) => (\n              <button\n                key={tab.id}\n                className={activeTab === tab.id ? \"active\" : \"\"}\n                onClick={() => setActiveTab(tab.id)}\n              >\n                {tab.label}\n              </button>\n            ))}\n          </div>\n          {activeTab === 0 && (\n            <div className=\"objective-content\">\n              <p>{objectives}</p>\n              {/* <input\n                type=\"text\"\n                value={objectives}\n                onChange={handleObjectiveChange}\n                readOnly={edit}\n              /> */}\n            </div>\n          )}\n          {activeTab === 1 && (\n            <div className=\"flex flex-col mt-[100px]\">\n              {userType && userType === USER_TYPE.INSTRUCTOR && (\n                <div className=\"flex justify-end\">\n                  <button onClick={toggleEditModal}>Add Modules</button>\n                </div>\n              )}\n              <CourseModules courseId={courseId} />\n            </div>\n          )}\n        </div>\n      </div>\n      {editModalVisible && (\n        <EditModal\n          setEditModalVisible={setEditModalVisible}\n          courseId={courseId}\n        />\n      )}\n    </>\n  );\n};\nexport default CourseObjective;\n\nconst CourseModules = ({ courseId }) => {\n  const { getCourseModules } = queries;\n\n  const getCourseModulesQuery = useQuery({\n    queryKey: [\"courses-modules\"],\n    queryFn: () => getCourseModules(courseId),\n  });\n\n  if (getCourseModulesQuery?.isLoading) {\n    return <div>Loading modules...</div>;\n  }\n\n  if (getCourseModulesQuery?.isError) {\n    return <>An error occurred while loading modules</>;\n  }\n\n  const modules = getCourseModulesQuery?.data?.data?.resource?.modules;\n\n  return (\n    <div className=\"outlineCard-container mt-4\">\n      <ul className=\"fcourse-container-grid\">\n        {\" \"}\n        {!!modules?.length &&\n          modules?.map((module, index) => (\n            <li key={index} className=\"border-none\">\n              <OutlineCard\n                index={index + 1}\n                moduleTitle={module?.title}\n                moduleId={module?._id}\n                courseId={courseId}\n                materials={module?.materials ?? []}\n              />\n            </li>\n          ))}{\" \"}\n      </ul>\n    </div>\n  );\n};\n","/Users/user/schoolbook-fe/src/component/Course Objective Tab/Video.jsx",[],[],"/Users/user/schoolbook-fe/src/store/materialStore.js",[],[],"/Users/user/schoolbook-fe/src/pages/DashboardPage/Dashboard.jsx",[],[],"/Users/user/schoolbook-fe/src/component/Courses/Course.jsx",[],[],"/Users/user/schoolbook-fe/src/component/Featured Courses/Fcourse.jsx",[],[],"/Users/user/schoolbook-fe/src/index.js",[],[],"/Users/user/schoolbook-fe/src/App.js",[],[],"/Users/user/schoolbook-fe/src/context/AuthProvider.js",[],[],"/Users/user/schoolbook-fe/src/store/store.js",[],[],"/Users/user/schoolbook-fe/src/component/PrivateRoutes.js",[],[],"/Users/user/schoolbook-fe/src/component/PublicRoutes.js",[],[],"/Users/user/schoolbook-fe/src/component/Register/InstructorLogin.jsx",[],[],"/Users/user/schoolbook-fe/src/component/Register/StudentLogin.jsx",[],[],"/Users/user/schoolbook-fe/src/pages/Instructor Dash/CreatedCourse.jsx",["457"],[],"import {React, useState} from 'react'\nimport '../DashboardPage/Dashboard.css'\nimport './InstructorDashboard.css'\nimport Nav from '../../component/Navbar/Nav'\nimport Create from '../../component/Create Courses/Create';\nimport Created from '../../component/Featured Courses/Created';\nimport OutlineCourse from '../../component/Course Component/OutlineCourse';\nimport { useSelector } from 'react-redux';\nimport { Link } from 'react-router-dom';\n\n\n\nconst tab = [\n  { id: 1, label: \"Home\" },\n  { id: 2, label: \"Create Course\" },\n  { id: 0, label: \"Course Created\" }\n]